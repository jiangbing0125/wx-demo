@import "list.wxss";
.container{
    background-color: #fff;
}
.ui-list .item .line-first{
    display: none;
}
.groupname{
    padding: 10rpx 40rpx;
    background-color: #f3f3f5;
    font-size: 32rpx;
}
.search-wrap{
    position: fixed;
    right: 0;
    top: 0;
    height: 100%;
    padding: 10rpx;
    display: flex;
    align-items: center;
    text-align: center;
}
.search-wrap .letter{
    flex: 1;
    width: 60rpx;
}
.search-wrap .letter view{
    color: #576b95;
    padding: 4rpx 0;
    font-size: 28rpx;
}